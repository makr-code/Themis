{
  "storage": {
  "rocksdb_path": "C:\\VCC\\themis\\data\\themis_server",
    "memtable_size_mb": 256,
    "block_cache_size_mb": 1024,
    "enable_blobdb": true,
    "compression": {
      "default": "lz4",
      "bottommost": "zstd"
    }
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8765,
    "worker_threads": 8
  },
  "vector_index": {
    "object_name": "documents",
    "dimension": 10,
    "metric": "COSINE",
    "engine": "hnsw",
    "hnsw_m": 16,
    "hnsw_ef_construction": 200,
    "ef_search": 64,
    "use_gpu": false,
    "auto_save": true,
    "save_path": "C:\\VCC\\themis\\data\\vector_indexes",
    "save_on_shutdown": true,
    "load_on_startup": true
  },
  "tracing": {
    "enabled": true,
    "service_name": "themis-server",
    "otlp_endpoint": "http://localhost:4318"
  },
  "features": {
    "semantic_cache": true,
    "llm_store": true,
    "cdc": true,
    "timeseries": true,
    "retention": {
      "enabled": true,
      "interval_hours": 24,
      "policies_path": "./config/retention_policies.yaml"
    }
  },
  "sse": {
    "max_events_per_second": 0
  }
}
